<Window x:Class="FarmProject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FarmProject"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <RadioButton x:Name="Admin" Content="Admin" HorizontalAlignment="Left" Margin="304,110,0,0" VerticalAlignment="Top" Height="24" Width="155" Checked="Admin_Checked"/>
        <RadioButton x:Name="Sales" Content="Sales" HorizontalAlignment="Left" Margin="304,184,0,0" VerticalAlignment="Top" Height="33" Width="116" FontSize="16" Checked="Sales_Checked"/>

    </Grid>
</Window>
